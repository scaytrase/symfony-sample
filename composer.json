{
  "name": "scaytrase/symfony-sample",
  "description": "Sample APP for learning",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Pavel Batanov",
      "email": "pavel@batanov.me"
    }
  ],
  "autoload": {
    "psr-4": {
      "ScayTrase\\Symfony\\Sample\\Kernel\\": "app/",
      "ScayTrase\\Symfony\\Sample\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "php": "^7.0",
    "symfony/symfony": "~3.2",
    "symfony/assetic-bundle": "~2.8",
    "doctrine/orm": "~2.5",
    "doctrine/doctrine-bundle": "~1.6",
    "sensio/framework-extra-bundle": "~3.0",
    "ramsey/uuid": "~3.5",
    "bower-asset/bootstrap": "^3.3",
    "bower-asset/jquery": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7"
  },
  "config": {
    "bin-dir": "bin/"
  }
}
