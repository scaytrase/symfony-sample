<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {% block head %}
        <title>{% block title %}{% endblock %}</title>
        {% block stylesheets %}
            {% stylesheets './../vendor/bower-asset/bootstrap/dist/css/bootstrap.css' %}
            <link href="{{ asset_url }}" rel="stylesheet">
            {% endstylesheets %}
            {% stylesheets '@BlogBundle/Resources/public/css/signin.css' %}
            <link href="{{ asset_url }}" rel="stylesheet">
            {% endstylesheets %}
        {% endblock %}
    {% endblock %}
</head>
<body>
{% block body %}
{% endblock %}

{% block javascripts %}
    {% javascripts './../vendor/bower-asset/jquery/dist/jquery.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    {% javascripts './../vendor/bower-asset/bootstrap/dist/js/bootstrap.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}
</body>
</html>
